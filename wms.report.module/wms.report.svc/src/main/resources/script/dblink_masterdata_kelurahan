AS SELECT result.id,
    result.createdby,
    result.createddate,
    result.modifiedby,
    result.modifieddate,
    result.kelurahanid,
    result.kelurahanname,
    result.kelurahanstatus,
    result.subzipcode,
    result.kecamatanid
   FROM dblink('dbname=hcis-masterdata host=128.199.186.186 port=5432 user=hcis password=snGCcNt0w2JGHQBrP2kaTlQVk7HlxVjy'::text, 'SELECT * FROM mst_kelurahan'::text) result(id bigint, createdby character varying(255), createddate timestamp without time zone, modifiedby character varying(255), modifieddate timestamp without time zone, kelurahanid character varying(255), kelurahanname character varying(255), kelurahanstatus character varying(255), subzipcode character varying(255), kecamatanid character varying(255));

