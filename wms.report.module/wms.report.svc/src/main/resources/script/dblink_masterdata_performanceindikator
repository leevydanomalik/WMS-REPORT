    result.performanceindicatorsubject,
    result.performanceindicatortype
   FROM dblink('dbname=hcis-masterdata host=128.199.186.186 port=5432 user=hcis password=snGCcNt0w2JGHQBrP2kaTlQVk7HlxVjy'::text, 'SELECT * FROM mst_performanceindicator'::text) result(id bigint, createdby character varying(255), createddate timestamp without time zone, modifiedby character varying(255), modifieddate timestamp without time zone, performanceindicator character varying(255), performanceindicatorid character varying(255), performanceindicatorstatus character varying(255), performanceindicatorsubject text, performanceindicatortype text);
