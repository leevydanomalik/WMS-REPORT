    result.employeeid,
    result.outsourceid,
    result.outsourcename,
    result.outsourcestatus
   FROM dblink('dbname=hcis-outsource host=128.199.186.186 port=5432 user=hcis password=snGCcNt0w2JGHQBrP2kaTlQVk7HlxVjy'::text, 'SELECT * FROM trx_outsourceentryprojection'::text) result(outsource_obj_disc character varying(255), id bigint, createdby character varying(255), createddate timestamp without time zone, recordid character varying(255), updatedby character varying(255), updateddate timestamp without time zone, employeeid character varying(255), outsourceid character varying(255), outsourcename character varying(255), outsourcestatus character varying(255));
