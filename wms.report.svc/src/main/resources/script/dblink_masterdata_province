CREATE OR REPLACE VIEW public.mst_province
AS SELECT result.id,
    result.createdby,
    result.createddate,
    result.modifiedby,
    result.modifieddate,
    result.provinceid,
    result.provincename,
    result.provincestatus,
    result.countryid
   FROM dblink('dbname=hcis-masterdata host=128.199.186.186 port=5432 user=hcis password=snGCcNt0w2JGHQBrP2kaTlQVk7HlxVjy'::text, 'SELECT * FROM mst_province'::text) result(id bigint, createdby character varying(255), createddate timestamp without time zone, modifiedby character varying(255), modifieddate timestamp without time zone, provinceid character varying(255), provincename character varying(255), provincestatus character varying(255), countryid character varying(255));
