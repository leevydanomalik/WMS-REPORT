<?xml version="1.0" encoding="UTF-8"?>
<ehcache
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://www.ehcache.org/ehcache.xsd"
        updateCheck="true"
        monitoring="autodetect"
        dynamicConfig="true">
    
<!--    FIND ALL CACHE-->
    <cache name="findOneByReportTemplateIDCache"
           maxElementsInMemory="1000000"
           eternal="false"
           overflowToDisk="true"
           timeToLiveSeconds="1"
           timeToIdleSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off" />

<!--    ADDRESS CACHE-->
    <cache name="findOneByReportDataIDCache"
            maxElementsInMemory="1000000"
            eternal = "false"
            overflowToDisk="true"
            timeToLiveSeconds="1"
            timeToIdleSeconds="0"
            memoryStoreEvictionPolicy="LFU"
            transactionalMode="off"/>
    
</ehcache>
